{
  "name": "confetti-coder",
  "displayName": "Confetti Coder",
  "description": "Celebrate your coding victories with a burst of virtual confetti as you type.",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other",
    "Visualization",
    "Themes",
    "Education"
  ],
  "keywords": [
    "confetti",
    "celebration",
    "fun",
    "motivation",
    "productivity"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "confetti-coder.showConfettiPlayground",
        "title": "Show Celebration Playground"
      },
      {
        "command": "confetti-coder.showConfettiDisplay",
        "title": "Show Confetti Display"
      },
      {
        "command": "confetti-coder.triggerConfetti",
        "title": "Trigger Celebration!"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
